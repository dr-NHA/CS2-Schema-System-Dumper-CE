CSGOSetCVarValue=function(Name,Value);
local CVarBase=ReadPointer("CVarPtr");
local Array=readPointer(CVarBase+0x40);
local Count=readInteger(CVarBase+0x48);
local Child=nil;
local ChildName=nil;
local Ind=0;
local Content=nil;
for Ind=0,Count,1 do
Child=readPointer(Array+(Ind*0x10));
if not (Child==nil or Child==0) then;
ChildName=readPointer(Child);
if not (ChildName==nil or ChildName==0) then;
ChildName=readString(ChildName);
if (not (ChildName==nil or #ChildName==0)) and Name==ChildName then;
--Increase Times Modified,
--[[NotNeeded?]]--writeInteger(Child+0x2C,readInteger(Child+0x2C)+1);
--Data Seems To Be At 40,
if type(true)=="boolean" then;
writeQword(Child+0x40,Value and 1 or 0);
elseif type(true)=="number" then;
writeQword(Child+0x40,Value);
else
writeInteger(Child+0x40,Value);
end;
break;
end;
end;
end;
end;
return Content;
end;
FNR("CSGOSetCVarValue");

end;

